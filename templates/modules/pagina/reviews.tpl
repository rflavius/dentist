<h2>Impresii, pareri despre platforma DentistOnline</h2>

<div class='row text-center'>
	<!-- BEGIN list_reviews -->
	<a name='review_{ID}'></a>
	<ul class='list-group col-xs-12'>
		<li class='list-group-item'>
			<p class='text-justify help-block'>
				<small>
					<a href='{CABINET_LINK}' target="_blank">{NAME}</a><br>
					Membru din: {START_DATE}
				</small>
			</p>
			<p class='small text-justify'>
				<a href='{CABINET_LINK}' target='_blank'><img src='{REVIEW_IMAGE}' class='img-responsive pull-left img-thumbnail' alt='imagine {NAME}' title='imagine {NAME}' style='max-width:40%;margin:0px 10px 10px 0px;'></a>
				<p class='text-left'>{REVIEW}</p>
			</p>
		</li>
	</ul>
	<!-- END list_reviews -->
</div>